@article{pomraning_higher_1996,
	title = {Higher {Order} {Fokker}-{Planck} {Operators}},
	volume = {124},
	issn = {0029-5639},
	url = {https://doi.org/10.13182/NSE96-A17918},
	doi = {10.13182/NSE96-A17918},
	abstract = {If the scattering interaction in linear particle transport problems is highly peaked about zero momentum transfer, a common and often useful approximation is the replacement of the integral scattering operator with the differential Fokker-Planck operator. This operator involves a first derivative in energy and second derivatives in angle. In this paper, higher order Fokker-Planck scattering operators are derived, involving higher derivatives in both energy and angle. The applicability of these higher order differential operators to representative scattering kernels is discussed. It is shown that, depending upon the details of the scattering kernel in the integral operator, higher order Fokker-Planck approximations may or may not be valid. Even the classic low-order Fokker-Planck operator fails as an approximation for certain highly peaked scattering kernels. In particular, no Fokker-Planck operator is a valid approximation for scattering involving the widely used Henyey-Greenstein scattering kernel.},
	number = {3},
	urldate = {2025-10-07},
	journal = {Nuclear Science and Engineering},
	author = {Pomraning, G. C.},
	month = nov,
	year = {1996},
	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.13182/NSE96-A17918},
	pages = {390--397},
	file = {Full Text PDF:/home/nglaser/Zotero/storage/L8SEDXFJ/Pomraning - 1996 - Higher Order Fokker-Planck Operators.pdf:application/pdf},
}

@inproceedings{beling_generalized_2023,
	address = {Niagara Falls, Ontario, Canada},
	title = {Generalized {Fokker}-{Planck} {Approximations} for {Fast}-{Ion} {Slowing} {Down}},
	abstract = {Moment-preserving Generalized Fokker-Planck (GFP) approximations for highly peaked angular scattering kernels have been previously developed and numerically demonstrated to be significantly more accurate than the Fokker-Planck (FP) approximation. Here this methodology is employed in constructing GFP approximations to highly peaked energy loss kernels relevant to fast-ion slowing down. Numerical solutions are obtained using space-energy discontinuous Galerkin discretizations and energy spectra and dose profiles for the FP and second and fourth-order GFP approximations are compared against Monte Carlo simulation of the exact problem. The fourth-order GFP approximation is seen to be extremely accurate relative to the FP approximation which moreover shows significant upscatter in energy.},
	language = {en},
