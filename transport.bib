	pages = {780--793},
	file = {Full Text PDF:/home/nglaser/Zotero/storage/FCTF5JUM/St. Aubin et al. - 2015 - A deterministic solution of the first order linear Boltzmann transport equation in the presence of e.pdf:application/pdf;Snapshot:/home/nglaser/Zotero/storage/KPHXTQDI/1.html:text/html},
